            {% else %}
                {{ site.name }}
            {% endif %}
        </title>
    <meta name="description" content="{{site.description}}">
    <link href="{{site.theme.link}}/images/logo4_1.png" rel="icon">
    <link rel="stylesheet" href="{{site.theme.link}}/css/screen.css" type="text/css" media="screen, projection" />
